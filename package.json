    "@mui/x-data-grid": "^6.19.2",
    "@tanstack/react-query": "^5.59.19",
    "@tanstack/react-query-devtools": "^5.17.9",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-async-handler": "^1.2.0",
