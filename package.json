{
  "name": "content-management-system",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node dist/server/index.js",
    "dev:server": "cross-env NODE_ENV=development nodemon",
    "dev:client": "vite",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
